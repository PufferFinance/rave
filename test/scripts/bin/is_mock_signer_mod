INPUT=$(./echo $(cat -) | ./trim_whitespace)
SIGNER_MOD=""

./echo $LEAF_SIGNER_MOD | ./is_valid_hex_len > /dev/null
./echo $INPUT | ./is_valid_hex_len > /dev/null

if [ "$INPUT" = "$SIGNER_MOD" ]; then
    ./echo '1'
else
    echo 'input signer mod not correct '$INPUT >> error.log
    ./echo '0'
fi